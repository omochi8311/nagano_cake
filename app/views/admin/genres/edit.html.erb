<div class="container">
  <div class="row my-3 h5">
    <h4 class="title">ジャンル編集</h4>
  </div>
    <div class="col-md-8">
      <%= form_with model: @genre,url: admin_genre_path(@genre) do |f| %>
        <h5>ジャンル名
          <%= f.text_field :name %>
          <%= f.submit "変更を保存",class: "btn btn-success" %>
        </h5>
      <% end %>
    </div>
</div>